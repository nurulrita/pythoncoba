nilai = input("Masukkan nilai anda:")
if nilai>80 and nilai<=100:
    print "Nilai Anda: A"
elif nilai<=79 and nilai>=60:
    print "Nilai Anda: B"
elif nilai<=59 and nilai>=40:
    print "Nilai Anda: C" 
elif nilai<=39 and nilai>=20:
    print "Nilai Anda: D"
elif nilai<=19 and nilai>=0:
    print "Nilai Anda: E"
elif nilai<0:
    print "Nilai Anda: ngaco"
elif nilai>100:
    print "Nilai Anda: mana mungkin bro"



